{
  "name": "@advclb/foobar",
  "version": "1.0.0",
  "description": "Foobar library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "start": "npm run doc:start",
    "build": "webpack --mode=production",
    "deploy": "npm run doc:deploy",
    "doc:start": "webpack-dev-server --mode=development --config=doc/webpack.config.js",
    "doc:build": "webpack --mode=production --config=doc/webpack.config.js",
    "doc:deploy": "gh-pages -d doc/dist",
    "test": "jest",
    "eslint": "eslint src/**/*.ts",
    "stylelint": "stylelint src/**/*.css",
    "prettier": "prettier --write src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/advclb/foobar.git"
  },
  "author": "Guo Yunhe",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/advclb/foobar/issues"
  },
  "homepage": "https://github.com/advclb/foobar#readme",
  "devDependencies": {
    "@advclb/stylelint-config": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "clean-webpack-plugin": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "eslint-config-airbnb": "latest",
    "eslint-config-prettier": "latest",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "gh-pages": "latest",
    "html-webpack-plugin": "latest",
    "jest": "latest",
    "prettier": "latest",
    "react": "latest",
    "react-dom": "latest",
    "sass": "latest",
    "sass-loader": "latest",
    "style-loader": "latest",
    "stylelint": "latest",
    "ts-loader": "latest",
    "typescript": "latest",
    "webpack": "latest",
    "webpack-cli": "latest",
    "webpack-dev-server": "latest"
  }
}
