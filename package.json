{
  "name": "@advclb/foobar",
  "version": "1.0.0",
  "description": "Foobar library",
  "main": "dist/foobar.umd.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "dist/*",
    "lib/*"
  ],
  "scripts": {
    "start": "npm run doc:start",
    "build": "tsc --declaration && webpack --mode=production",
    "doc:start": "webpack-dev-server --mode=development --config=doc/webpack.config.js",
    "doc:build": "webpack --mode=production --config=doc/webpack.config.js",
    "test": "jest",
    "eslint": "eslint src/**/*.ts",
    "stylelint": "stylelint src/**/*.css",
    "prettier": "prettier --write src/**"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/advclb/foobar.git"
  },
  "author": "Guo Yunhe",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/advclb/foobar/issues"
  },
  "homepage": "https://github.com/advclb/foobar#readme",
  "devDependencies": {
    "@advclb/eslint-config": "^18.0.2",
    "@advclb/stylelint-config": "^0.1.1",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/parser": "^2.22.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.1",
    "prettier-stylelint": "^0.4.2",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "sass": "^1.26.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.14.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.3",
